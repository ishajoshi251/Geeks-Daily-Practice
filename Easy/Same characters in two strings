class Solution:

    def sameChar(self, A, B):
        # code here
        A = A.lower()
        B = B.lower()
        count = 0
        i = 0
        j = 0
        while i<len(A) and j<len(B):
            if A[i] == B[j]:
                count += 1
            i += 1
            j += 1
        return count
