class Solution:

	
	def numOfsubarrays(self,arr, n):
    	# code here
        sum = 0
        count = 0
        prod = 1
        for i in range(n):
            sum += arr[i]
            prod *= arr[i]
            for j in range(i+1,n):
                sum += arr[j]
                prod *= arr[j]
                if sum == prod:
                    count += 1
            sum = 0
            prod = 1
        return n+count
