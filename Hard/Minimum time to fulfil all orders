from typing import List
class Solution:
    def findMinTime(self, n : int, l : int, arr : List[int]) -> int:
        # code here
        ans = []
        for i in arr:
            ele = i
            ans.append(i)
            for i in range(2,n+1):
                ans.append(ans[-1] + (ele*i))
        ans.sort()
        return ans[n-1]
  
