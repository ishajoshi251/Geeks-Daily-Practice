def findEquilibrium(A,N):
    # Code here
     # Your code here
       larr=0
       rarr=sum(A)-A[0]
       
       if(len(A)==1):
           return 0
       
       for i in range(0,N-1):
           if(larr==rarr):
               return i
           
           larr+=A[i]
           rarr-=A[i+1]
           
       return -1
